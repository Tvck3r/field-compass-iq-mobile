# API Configuration
API_URL=http://localhost:5000
WS_URL=ws://localhost:5000/hubs

# Maps Configuration
MAPS_API_KEY=your_google_maps_api_key
MAPS_PROVIDER=google

# Push Notifications
PUSH_SENDER_ID=your_firebase_sender_id
FCM_SERVER_KEY=your_fcm_server_key

# Application Configuration
APP_NAME=Field-Compass-IQ
APP_VERSION=1.0.0
BUNDLE_ID=com.fieldcompass.mobile

# Development
DEV_MODE=true
LOG_LEVEL=debug
FLIPPER_ENABLED=true

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_CAMERA=true
ENABLE_GPS=true
ENABLE_PUSH_NOTIFICATIONS=true