# API Configuration
API_URL=http://localhost:5000
WS_URL=ws://localhost:5000/hubs

# Maps Configuration
MAPS_API_KEY=your_google_maps_api_key
MAPS_PROVIDER=google

# Push Notifications
PUSH_SENDER_ID=your_fcm_sender_id
PUSH_SERVER_KEY=your_fcm_server_key

# App Configuration
APP_NAME=Field-Compass-IQ
APP_VERSION=1.0.0

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_BIOMETRIC_AUTH=true
ENABLE_CAMERA=true
ENABLE_LOCATION=true
ENABLE_PUSH_NOTIFICATIONS=true

# Development
DEV_MODE=true
LOG_LEVEL=debug
FLIPPER_ENABLED=true